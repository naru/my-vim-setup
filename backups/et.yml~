##Kuupäeva ja aja vorming
  date:
    formats:
      default: "%B %d, %Y"

    day_names: [pühapäev, esmaspäev, teisipäev, kolmapäev, neljapäev, reede, laupäev]
    standalone_day_names: [Pühapäev, Esmaspäev, Teisipäev, Kolmapäev, Neljapäev, Reede, Laupäev]
    abbr_day_names: [P, E, T, K, N, R, L]

    month_names: [~, Jaanuar, Veebruar, Märts, Aprill, Mai, Juuni, Juuli, August, September, Oktoober, November, Detsember]
    standalone_month_names: [~, Jaanuar, Veebruar, Märts, Aprill, Mai, Juuni, Juuli, August, September, Oktoober, November, Detsember]
    abbr_month_names: [~, Jaan., veebr., märts, aprill, mai, juuni, juuli, august, september, okt., november, dets.]
    standalone_abbr_month_names: [~, Jaan., veebr., märts, aprill, mai, juuni, juuli, august, september, okt., november, dets.]

    order: [ :day, :month, :year ]

  time:
    formats:
      default: "%B %d, %Y - %H:%M"
    am: "Enne lõunat"
    pm: "Pärast lõunat"

  number:
    format:
      separator: ","
      delimiter: ""
      precision: 2

    Currency:
      format:
        format: "% n% u"
        unit: "kr"
        separator: ","
        delimiter: ""
        precision: 2

    percentage:
      format:
        delimiter: ""

    precision:
      format:
        delimiter: ""

    human:
      format:
        delimiter: ""
        precision: 1
      storage_units: [söödaks, KB, MB, GB, TB]

  datetime:
    distance_in_words:
      half_a_minute: "bassein minutit"
      less_than_x_seconds:
        one: "Vähem OTS ((arv)) sekund"
        other: "Vähem OTS ((arv)) sekundit"
      x_seconds:
        one: "((arv)) sekund"
        other: "((arv)) sekundit"
